      "temperature": 0.07,
      "base_temperature": 0.07,
      "max_samples": 4096,
      "max_views": 1000,
      "stride": 8,
      "loss_weight": 0.3,
      "use_rmi": false,
      "use_lovasz": false,
      "with_memory": true,
      "memory_size": 5000,
      "pixel_update_freq": 10,
      "coefficient": 0.999,
      "upsample": "before"
